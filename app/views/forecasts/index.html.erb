<p style="color: green"><%= notice %></p>

<h1>Forecasts</h1>

<%= render "form" %>
<%= turbo_stream_from "forecasts" %>
<div id="forecasts">
  <% @forecasts.each do |forecast| %>
    <%= render forecast %>
    <p>
      <%= link_to "Show this forecast", forecast %>
    </p>
  <% end %>
</div>

